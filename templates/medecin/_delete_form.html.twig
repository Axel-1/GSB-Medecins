<form style="display: inline;" method="post" action="{{ path('medecin_delete', {'id': medecin.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ medecin.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
