<form style="display: inline;" method="post" action="{{ path('departement_delete', {'id': departement.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ departement.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
